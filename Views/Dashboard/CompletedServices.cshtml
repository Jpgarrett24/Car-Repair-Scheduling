@model Wrapper
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Completed Services</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
    <style>
        body{
            padding: 1% 5%;
        }
    </style>
</head>
<body>
    <header>
        <h1 class="text-center">Completed Services</h1>
        <ul class="nav nav-pills nav-fill">
            <li class="nav-item">
                <a class="nav-link active" href="/dashboard">Home</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/service">Services</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/service/completed">Completed Services</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/logout">Log Out</a>
            </li>
        </ul>
    </header>
    <main>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Service</th>
                    <th scope="col">Customer</th>
                    <th scope="col">Vehicle</th>
                    <th scope="col">Mileage</th>
                    <th scope="col">Time of Service</th>
                    <th scope="col">Price</th>
                </tr>
            </thead>
            <tbody>
                @foreach (Service service in Model.AllServices)
                {
                    <tr>
                        <td>@service.ServiceType.Name</td>
                        <td>@service.ServicedCar.Owner.FirstName @service.ServicedCar.Owner.LastName</td>
                        <td>@service.ServicedCar.Year @service.ServicedCar.Make @service.ServicedCar.Model</td>
                        <td>@service.ServicedCar.Mileage</td>
                        <td>@service.Start</td>
                        <td>@service.Price</td>
                    </tr>
                }
            </tbody>
        </table>
    </main>
</body>
</html>